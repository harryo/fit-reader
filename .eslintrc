{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "rules": {
    "func-names": ["error", "never"],
    "no-plusplus": 0,
    "import/prefer-default-export": 0,
    "max-len": 0,
    "no-use-before-define": 0,
  }
}
